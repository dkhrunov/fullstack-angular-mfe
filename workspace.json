{
	"version": 2,
	"projects": {
		"api": {
			"root": "apps/api",
			"sourceRoot": "apps/api/src",
			"projectType": "application",
			"targets": {
				"build": {
					"executor": "@nrwl/node:build",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/api",
						"main": "apps/api/src/main.ts",
						"tsConfig": "apps/api/tsconfig.app.json",
						"assets": ["apps/api/src/assets"]
					},
					"configurations": {
						"production": {
							"optimization": true,
							"extractLicenses": true,
							"inspect": false,
							"fileReplacements": [
								{
									"replace": "apps/api/src/environments/environment.ts",
									"with": "apps/api/src/environments/environment.prod.ts"
								}
							]
						}
					}
				},
				"serve": {
					"executor": "@nrwl/node:execute",
					"options": {
						"buildTarget": "api:build"
					}
				},
				"lint": {
					"executor": "@nrwl/linter:eslint",
					"outputs": ["{options.outputFile}"],
					"options": {
						"lintFilePatterns": ["apps/api/**/*.ts"]
					}
				},
				"test": {
					"executor": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/api"],
					"options": {
						"jestConfig": "apps/api/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"dashboard": {
			"projectType": "application",
			"root": "apps/dashboard",
			"sourceRoot": "apps/dashboard/src",
			"prefix": "nx-mfe",
			"targets": {
				"build": {
					"executor": "@nrwl/angular:webpack-browser",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/dashboard",
						"index": "apps/dashboard/src/index.html",
						"main": "apps/dashboard/src/main.ts",
						"polyfills": "apps/dashboard/src/polyfills.ts",
						"tsConfig": "apps/dashboard/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"apps/dashboard/src/favicon.ico",
							"apps/dashboard/src/assets",
							{
								"glob": "**/*",
								"input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
								"output": "/assets/"
							}
						],
						"styles": [
							"apps/dashboard/src/styles.scss",
							"node_modules/ng-zorro-antd/ng-zorro-antd.min.css"
						],
						"scripts": [],
						"customWebpackConfig": {
							"path": "apps/dashboard/webpack.config.js"
						}
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "any@nrwl/angular:webpack-browserComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/dashboard/src/environments/environment.ts",
									"with": "apps/dashboard/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"customWebpackConfig": {
								"path": "apps/dashboard/webpack.prod.config.js"
							}
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"executor": "@nrwl/angular:webpack-server",
					"configurations": {
						"production": {
							"browserTarget": "dashboard:build:production"
						},
						"development": {
							"browserTarget": "dashboard:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {}
				},
				"extract-i18n": {
					"executor": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "dashboard:build"
					}
				},
				"lint": {
					"executor": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/dashboard/src/**/*.ts",
							"apps/dashboard/src/**/*.html"
						]
					}
				},
				"test": {
					"executor": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/dashboard"],
					"options": {
						"jestConfig": "apps/dashboard/jest.config.js",
						"passWithNoTests": true
					}
				},
				"serve-mfe": {
					"executor": "@nrwl/workspace:run-commands",
					"options": {
						"commands": [
							"nx serve login",
							"nx serve login",
							"nx serve dashboard"
						]
					}
				}
			}
		},
		"dashboard-e2e": {
			"root": "apps/dashboard-e2e",
			"sourceRoot": "apps/dashboard-e2e/src",
			"projectType": "application",
			"targets": {
				"e2e": {
					"executor": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/dashboard-e2e/cypress.json",
						"devServerTarget": "dashboard:serve:development",
						"tsConfig": "apps/dashboard-e2e/tsconfig.json"
					},
					"configurations": {
						"production": {
							"devServerTarget": "dashboard:serve:production"
						}
					}
				},
				"lint": {
					"executor": "@nrwl/linter:eslint",
					"outputs": ["{options.outputFile}"],
					"options": {
						"lintFilePatterns": ["apps/dashboard-e2e/**/*.{js,ts}"]
					}
				}
			}
		},
		"login": {
			"projectType": "application",
			"root": "apps/login",
			"sourceRoot": "apps/login/src",
			"prefix": "nx-mfe",
			"targets": {
				"build": {
					"executor": "@nrwl/angular:webpack-browser",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/login",
						"index": "apps/login/src/index.html",
						"main": "apps/login/src/main.ts",
						"polyfills": "apps/login/src/polyfills.ts",
						"tsConfig": "apps/login/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"apps/login/src/favicon.ico",
							"apps/login/src/assets",
							{
								"glob": "**/*",
								"input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
								"output": "/assets/"
							}
						],
						"styles": [
							"apps/login/src/styles.scss",
							"node_modules/ng-zorro-antd/ng-zorro-antd.min.css"
						],
						"scripts": [],
						"customWebpackConfig": {
							"path": "apps/login/webpack.config.js"
						}
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/login/src/environments/environment.ts",
									"with": "apps/login/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"customWebpackConfig": {
								"path": "apps/login/webpack.prod.config.js"
							}
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"executor": "@nrwl/angular:webpack-server",
					"configurations": {
						"production": {
							"browserTarget": "login:build:production"
						},
						"development": {
							"browserTarget": "login:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4201
					}
				},
				"extract-i18n": {
					"executor": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "login:build"
					}
				},
				"lint": {
					"executor": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/login/src/**/*.ts",
							"apps/login/src/**/*.html"
						]
					}
				},
				"test": {
					"executor": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/login"],
					"options": {
						"jestConfig": "apps/login/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"login-e2e": {
			"root": "apps/login-e2e",
			"sourceRoot": "apps/login-e2e/src",
			"projectType": "application",
			"targets": {
				"e2e": {
					"executor": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/login-e2e/cypress.json",
						"devServerTarget": "login:serve:development"
					},
					"configurations": {
						"production": {
							"devServerTarget": "login:serve:production"
						}
					}
				},
				"lint": {
					"executor": "@nrwl/linter:eslint",
					"outputs": ["{options.outputFile}"],
					"options": {
						"lintFilePatterns": ["apps/login-e2e/**/*.{js,ts}"]
					}
				}
			}
		},
		"login-form": {
			"projectType": "library",
			"root": "libs/login/form",
			"sourceRoot": "libs/login/form/src",
			"prefix": "nx-mfe",
			"targets": {
				"test": {
					"executor": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/login/form"],
					"options": {
						"jestConfig": "libs/login/form/jest.config.js",
						"passWithNoTests": true
					}
				},
				"lint": {
					"executor": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"libs/login/form/src/**/*.ts",
							"libs/login/form/src/**/*.html"
						]
					}
				}
			}
		},
		"shared-data-access-user": {
			"projectType": "library",
			"root": "libs/shared/data-access-user",
			"sourceRoot": "libs/shared/data-access-user/src",
			"prefix": "nx-mfe",
			"targets": {
				"test": {
					"executor": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/shared/data-access-user"],
					"options": {
						"jestConfig": "libs/shared/data-access-user/jest.config.js",
						"passWithNoTests": true
					}
				},
				"lint": {
					"executor": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"libs/shared/data-access-user/src/**/*.ts",
							"libs/shared/data-access-user/src/**/*.html"
						]
					}
				}
			}
		}
	},
	"cli": {
		"defaultCollection": "@nrwl/angular"
	},
	"generators": {
		"@nrwl/angular:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/angular:library": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest"
		},
		"@nrwl/angular:component": {
			"style": "scss"
		}
	},
	"defaultProject": "dashboard"
}
